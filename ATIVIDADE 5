programa
{
	
	funcao inicio()
	{
	inteiro num[10]
		escreva("digite 10 numeros inteiros ")


		
		para(inteiro i=0; i < 10; i++){
			escreva("digite 10 numeros inteiros ")
			leia(num[i])

		
	 }
	 para(inteiro i=0; i < 10; i++){
			se (num[i]%2==0){
				escreva("O numero par ",num[i],"está na posição ",i,"do vetor.","\n")
			}
		}
   }
}
